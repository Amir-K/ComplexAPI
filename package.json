{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean": "rimraf .next",
    "build": "npm run clean && rimraf sourcemaps && yalc update && next build --no-lint",
    "build:local": "npm run build && npm run copy-sourcemaps",
    "start": "next start",
    "lint": "next lint",
    "copy-sourcemaps": "rimraf sourcemaps && robocopy .next\\server sourcemaps *.map /E /XD node_modules",
    "buildstart": "npm run build:local && npm run start",
    "use:yalc": "yalc add @return-0/node && npm install",
    "use:npm": "yalc remove @return-0/node && npm install"
  },
  "dependencies": {
    "@return-0/node": "1.0.1",
    "next": "15.3.2",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/next": "^8.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
